#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x10fc210 .scope module, "imem_tb" "imem_tb" 2 1;
 .timescale 0 0;
P_0x10fdaf8 .param/l "BUS_WIDTH" 2 4, +C4<01010>;
P_0x10fdb20 .param/str "CODE_FILE" 2 5, "mips/benchmark.hex";
P_0x10fdb48 .param/l "DATA_WIDTH" 2 3, +C4<0100000>;
v0x11455b0_0 .var "addr", 31 0;
v0x1145670_0 .var "clk", 0 0;
v0x1145710_0 .net "rdata", 31 0, L_0x11458c0; 1 drivers
E_0x10ffed0 .event posedge, v0x1145670_0;
L_0x11459c0 .part v0x11455b0_0, 2, 10;
S_0x10fc350 .scope module, "imem" "imem" 2 15, 3 10, S_0x10fc210;
 .timescale 0 0;
P_0x10fdbd8 .param/l "BUS_WIDTH" 3 11, +C4<01010>;
P_0x10fdc00 .param/str "CODE_FILE" 3 11, "mips/benchmark.hex";
P_0x10fdc28 .param/l "DATA_WIDTH" 3 11, +C4<0100000>;
L_0x11458c0 .functor BUFZ 32, L_0x11457c0, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>;
v0x10fd9f0 .array "ROM", 1023 0, 31 0;
v0x10fc5f0_0 .net *"_s0", 31 0, L_0x11457c0; 1 drivers
v0x1145460_0 .net "addr", 9 0, L_0x11459c0; 1 drivers
v0x1145500_0 .alias "rdata", 31 0, v0x1145710_0;
L_0x11457c0 .array/port v0x10fd9f0, L_0x11459c0;
    .scope S_0x10fc350;
T_0 ;
    %vpi_call 3 20 "$readmemh", P_0x10fdc00, v0x10fd9f0, 1'sb0, 11'sb01111111111;
    %end;
    .thread T_0;
    .scope S_0x10fc210;
T_1 ;
    %load/v 8, v0x1145670_0, 1;
    %inv 8, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1145670_0, 0, 8;
    %delay 5, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x10fc210;
T_2 ;
    %vpi_call 2 26 "$dumpfile", "vcd/imem_tb.vcd";
    %vpi_call 2 27 "$dumpvars", 1'sb0, S_0x10fc210;
    %vpi_call 2 29 "$display", "addr,\011\011rdata";
    %vpi_call 2 30 "$monitor", "%x,\011%x", v0x11455b0_0, v0x1145710_0;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1145670_0, 0, 0;
    %ix/load 0, 32, 0;
    %assign/v0 v0x11455b0_0, 0, 0;
    %wait E_0x10ffed0;
    %movi 8, 4, 32;
    %ix/load 0, 32, 0;
    %assign/v0 v0x11455b0_0, 0, 8;
    %wait E_0x10ffed0;
    %movi 8, 44, 32;
    %ix/load 0, 32, 0;
    %assign/v0 v0x11455b0_0, 0, 8;
    %wait E_0x10ffed0;
    %movi 8, 64, 32;
    %ix/load 0, 32, 0;
    %assign/v0 v0x11455b0_0, 0, 8;
    %vpi_call 2 47 "$finish";
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "test/imem_tb.v";
    "src/imem.v";
