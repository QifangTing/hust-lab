#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xb6c210 .scope module, "imem_tb" "imem_tb" 2 1;
 .timescale 0 0;
P_0xb6daf8 .param/l "BUS_WIDTH" 2 4, +C4<01010>;
P_0xb6db20 .param/str "CODE_FILE" 2 5, "mips/benchmark.hex";
P_0xb6db48 .param/l "DATA_WIDTH" 2 3, +C4<0100000>;
v0xbb5b90_0 .var "addr", 31 0;
v0xbb5c50_0 .var "clk", 0 0;
v0xbb5cf0_0 .net "rdata", 31 0, L_0xbb5ea0; 1 drivers
E_0xb6fed0 .event posedge, v0xbb5c50_0;
L_0xbb5fa0 .part v0xbb5b90_0, 2, 10;
S_0xb6c350 .scope module, "imem" "imem" 2 15, 3 10, S_0xb6c210;
 .timescale 0 0;
P_0xb6dbd8 .param/l "BUS_WIDTH" 3 11, +C4<01010>;
P_0xb6dc00 .param/str "CODE_FILE" 3 11, "mips/benchmark.hex";
P_0xb6dc28 .param/l "DATA_WIDTH" 3 11, +C4<0100000>;
L_0xbb5ea0 .functor BUFZ 32, L_0xbb5da0, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>;
v0xb6d9f0 .array "ROM", 1023 0, 31 0;
v0xb6c5f0_0 .net *"_s0", 31 0, L_0xbb5da0; 1 drivers
v0xbb5a40_0 .net "addr", 9 0, L_0xbb5fa0; 1 drivers
v0xbb5ae0_0 .alias "rdata", 31 0, v0xbb5cf0_0;
L_0xbb5da0 .array/port v0xb6d9f0, L_0xbb5fa0;
    .scope S_0xb6c350;
T_0 ;
    %vpi_call 3 20 "$readmemh", P_0xb6dc00, v0xb6d9f0, 1'sb0, 11'sb01111111111;
    %end;
    .thread T_0;
    .scope S_0xb6c210;
T_1 ;
    %load/v 8, v0xbb5c50_0, 1;
    %inv 8, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0xbb5c50_0, 0, 8;
    %delay 5, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0xb6c210;
T_2 ;
    %vpi_call 2 26 "$dumpfile", "vcd/imem_tb.vcd";
    %vpi_call 2 27 "$dumpvars", 1'sb0, S_0xb6c210;
    %vpi_call 2 29 "$display", "addr,\011\011rdata";
    %vpi_call 2 30 "$monitor", "%x,\011%x", v0xbb5b90_0, v0xbb5cf0_0;
    %ix/load 0, 1, 0;
    %assign/v0 v0xbb5c50_0, 0, 0;
    %ix/load 0, 32, 0;
    %assign/v0 v0xbb5b90_0, 0, 0;
    %wait E_0xb6fed0;
    %movi 8, 4, 32;
    %ix/load 0, 32, 0;
    %assign/v0 v0xbb5b90_0, 0, 8;
    %wait E_0xb6fed0;
    %movi 8, 44, 32;
    %ix/load 0, 32, 0;
    %assign/v0 v0xbb5b90_0, 0, 8;
    %wait E_0xb6fed0;
    %movi 8, 64, 32;
    %ix/load 0, 32, 0;
    %assign/v0 v0xbb5b90_0, 0, 8;
    %vpi_call 2 47 "$finish";
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "test/imem_tb.v";
    "src/imem.v";
